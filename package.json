{
  "name": "nestjs-boilerplate",
  "version": "0.1.0",
  "description": "NestJS Boilerplate, Typescript, Postgres",
  "author": "Abbas Srour <abbas.mj.srour@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "console": "node ./dist/console.js",
    "build:prod": "nest build",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "start:dev": "nest start --watch",
    "start:stage": "ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "migration:generate": "mikro-orm migration:create",
    "migration:create": "mikro-orm migration:create --blank",
    "migration:update": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:fresh": "mikro-orm migration:fresh",
    "schema:fresh": "mikro-orm schema:fresh",
    "schema:drop": "mikro-orm schema:drop --drop-migrations-table",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "new": "node ./scripts/generate-module.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.731.1",
    "@langchain/community": "^0.3.25",
    "@langchain/core": "^0.3.31",
    "@langchain/langgraph": "^0.2.41",
    "@langchain/openai": "^0.3.17",
    "@mikro-orm/cli": "^6.4.3",
    "@mikro-orm/core": "^6.4.3",
    "@mikro-orm/entity-generator": "^6.4.3",
    "@mikro-orm/knex": "^6.4.3",
    "@mikro-orm/migrations": "^6.4.3",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/postgresql": "^6.4.3",
    "@mikro-orm/reflection": "^6.4.3",
    "@mikro-orm/seeder": "^6.4.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.1",
    "@nestjs/passport": "^11.0.2",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.3.0",
    "@scalar/nestjs-api-reference": "^0.3.175",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.11",
    "libphonenumber-js": "^1.11.18",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nest-commander": "^3.15.0",
    "nestjs-cls": "^4.1.0",
    "nestjs-i18n": "^10.4.5",
    "parse-duration": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.6.2",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/luxon": "^3.4.2",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^3.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-canonical": "^5.1.3",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "inquirer": "^12.3.2",
    "jest": "^29.7.0",
    "lint-staged": "~15.4.1",
    "prettier": "^3.2.5",
    "release-it": "^18.1.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^3.0.2"
  },
  "volta": {
    "node": "21.7.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
